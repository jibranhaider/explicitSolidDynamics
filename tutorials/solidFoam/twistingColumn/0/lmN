FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    location    "0";
    object      lmN;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -2 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    topAndSides
    {
        type    zeroGradient;
    }
    bottom
    {
        type    fixedValue;
        value   uniform (0 0 0);
    }
}

