FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

gradSchemes
{
    default     leastSquares;
}

divSchemes
{
    default     none;
}

laplacianSchemes
{
    default     none;
}

interpolationSchemes
{
    default     linear;
}

