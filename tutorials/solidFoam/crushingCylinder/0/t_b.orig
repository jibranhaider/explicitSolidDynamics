FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      t_b;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "../constant/runParameters"
#include "../constant/mechanicalProperties"

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    fixed
    {
        type            movingTraction;
        value           uniform (0 0 0);
    }
    free
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    symmetric
    {
        type            symmetricTraction;
        value           uniform (0 0 0);
    }
    loading
    {
        type            movingDisplacementTraction;
        density         $rho;
        displacement    $uMax;
        endTime         $tEnd;
        value           uniform (0 0 0);
    }
}

